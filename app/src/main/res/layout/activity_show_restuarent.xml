<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.snackhoop.mealsonwheels.ShowRestaurentActivity.ShowRestuarentActivity">
    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:background="@color/colorPrimary"
        app:title="Nearby restuarent"
        app:titleTextColor="@color/white"
        app:navigationIcon="@drawable/ic_close_white_36dp"
        android:layout_height="?attr/actionBarSize">

    </android.support.v7.widget.Toolbar>
<android.support.v7.widget.RecyclerView
    android:layout_width="match_parent"
    android:id="@+id/recyclerview_listrestaurent"
    android:layout_marginTop="?attr/actionBarSize"
    android:layout_height="match_parent">

</android.support.v7.widget.RecyclerView>
</RelativeLayout>
